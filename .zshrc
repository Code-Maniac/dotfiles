# Path to your oh-my-zsh installation.
# export ZSH=/home/nick/.oh-my-zsh

# set theme of the command line.
#ZSH_THEME="agnoster"

export PATH="/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/games:/usr/local/games"

# load plugin manager and oh-my-zsh
source ~/.antigen/antigen.zsh
antigen use oh-my-zsh

# theme
antigen theme agnoster

# antigen is finished
antigen apply

#instead of using cp use rsync instead.
alias cp="rsync -avz"

vim_man()
{
	vim -c "Man $@" -c 'silent only'
}

# redirect man to vim-man
alias man=vim_man

# set auto completion of commands
#setopt AUTO_CD

stty -ixon

[ -e ~/.dircolors ] && eval $(dircolors -b ~/.dircolors) || eval $(dircolors -b)

export TERM=xterm-256color

if [ "$TMUX" = "" ]; then tmux attach || tmux new; fi

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
